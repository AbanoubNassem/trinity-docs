{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj",
            "src/**.xml"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "trinity/**.md",
          "trinity/**/toc.yml",
          "trinity-table/**.md",
          "trinity-table/**/toc.yml",
          "trinity-form/**.md",
          "trinity-form/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "cleanupCacheHistory": true,
    "globalMetadata": {
      "_appTitle": "Trinity",
      "_appFooter": "Trinity Copyright Â© 2022 - 2023",
      "_appLogoPath": "./images/logo.png",
      "_appFaviconPath": "./images/favicon.ico",
      "_enableSearch": true,
      "rootNamespace": "Trinity",
      "_description": "Trinity is a feature-rich and beautifully-designed Single-Page Application (SPA) administration tool built using C# and ASP.NET. It is designed to streamline common administrative tasks and enhance the productivity of developers, making it easy to manage your website's backend with ease.",
      "_keywords": [
        "trinity",
        "admin panel",
        "administration tool",
        "C#",
        "ASP.NET",
        "Dapper",
        "SqlKata",
        "FluentValidation",
        "MiniProfiler",
        "ReactJS",
        "PrimeReact",
        "InertiaJS",
        "Vite",
        "SPA"
      ]
    },
    "sitemap": {
      "baseUrl": "https://trinity.abanoubnassem.com/docs",
      "priority": 0.1,
      "changefreq": "weekly"
    },
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}